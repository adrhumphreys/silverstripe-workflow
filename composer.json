{
    "name": "silverstripe/workflow",
    "description": "Workflows that support integration with third party managers.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4",
        "silverstripe/admin": "^1",
        "silverstripe/colorpicker": "1.x-dev",
        "silverstripe/iconpicker": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adrhumphreys/silverstripe-iconpicker.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/SomarDesignStudios/silverstripe-colorpicker.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "SilverStripe\\Workflow\\": "src/",
            "SilverStripe\\Workflow\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/assets"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
